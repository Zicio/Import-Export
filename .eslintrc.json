{
  "extends": "airbnb-base",
  "env": {
     "es6": true,
     "browser": true,
     "jest": true
  },
  "rules": {
    "no-restricted-syntax": [
      "error",
      "LabeledStatement",
      "WithStatement"
    ],
    "no-unused-vars": "warn",
    "no-multiple-empty-lines": "warn",
    "class-methods-use-this": ["error", { "exceptMethods": ["start"] }]
  }
}